{
  "overrides": [
    {
      "files": "*",
      "options": {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.css",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.html",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
