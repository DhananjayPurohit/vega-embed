{
  "name": "vega-embed",
  "version": "3.0.0-beta.6",
  "description": "Publish Vega visualizations as embedded web components.",
  "keywords": [
    "vega",
    "data",
    "visualization",
    "component",
    "embed"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-embed.git"
  },
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [{
    "name": "Jeffrey Heer",
    "url": "https://homes.cs.washington.edu/~jheer/"
  }, {
    "name": "Arvind Satyanarayan",
    "url": "http://arvindsatya.com"
  }, {
    "name": "Younghoon Kim"
  }, {
    "name": "Dominik Moritz",
    "url": "https://www.domoritz.de"
  }],
  "license": "BSD-3-Clause",
  "main": "src/embed.js",
  "devDependencies": {
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "eslint": "^3.17.1",
    "uglify-js": "^2.8.11",
    "watchify": "^3.9.0",
    "vega": "^3.0.0-beta.25",
    "vega-lite": "^2.0.0-alpha.4"
  },
  "dependencies": {
    "d3-selection": "^1.0.5",
    "vega-schema-url-parser": "^1.0.0-beta.2"
  },
  "scripts": {
    "deploy": "npm run lint && scripts/deploy.sh",
    "lint": "eslint src/",
    "build": "browserify src/embed.js -d -s vega.embed > vega-embed.js",
    "postbuild": "uglifyjs vega-embed.js -cm > vega-embed.min.js",
    "watch": "watchify src/embed.js -v -d -s vega.embed -o vega-embed.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "vega": "global:vega",
    "vega-lite": "global:vl"
  }
}